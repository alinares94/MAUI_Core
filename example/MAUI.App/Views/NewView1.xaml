<?xml version="1.0" encoding="utf-8" ?>
<base:PageBase 
    xmlns:base="clr-namespace:MAUI.Core.Views;assembly=MAUI.Core" 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="MAUI.App.Views.NewView1"
    Title="NewPage1">
    <VerticalStackLayout>
        <Label 
            Text="Welcome to .NET MAUI! Page 1"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />
        <Button
            Text="Navigate to Page 2"
            Command="{Binding GoToView2Command}"/>
    </VerticalStackLayout>
</base:PageBase>